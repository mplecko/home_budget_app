# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
!/tmp/.keep
/bin

# Ignore uploaded files in development.
/storage/*
!/storage/.keep

# Ignore credentials.
/config/credentials.yml.enc
/config/master.key

# Ignore any secret files
.env

# Ignore files generated by the debugger.
/byebug_history

# Ignore database config
/config/database.yml

# Ignore master key for decrypting credentials and more.
/config/master.key
